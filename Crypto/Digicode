def extraire_blocs(seq, n=4):
    """
    Extrait tous les blocs de n chiffres d'une séquence cyclique.
    Pour chaque position de la chaîne, on prend n chiffres en
    "rebouclant" au début si nécessaire.
    """
    L = len(seq)
    blocs = set()
    for i in range(L):
        # Concatène la fin avec le début si nécessaire
        if i + n <= L:
            bloc = seq[i:i + n]
        else:
            bloc = seq[i:] + seq[:(i + n - L)]
        blocs.add(bloc)
    return blocs


def main():
    # Lecture de la séquence depuis le fichier
    with open("digicode.txt", "r") as f:
        # On supprime les espaces, retours à la ligne, etc.
        seq = f.read().strip().replace("\n", "")

    # Extraction des combinaisons présentes dans la séquence
    blocs = extraire_blocs(seq, 4)

    # Création de l'ensemble de toutes les combinaisons possibles de 0000 à 9999
    toutes_combi = {f"{i:04d}" for i in range(10000)}

    # Calcul des combinaisons manquantes
    manquantes = sorted(toutes_combi - blocs)

    # Affichage des résultats séparés par un espace
    print(" ".join(manquantes))


if __name__ == "__main__":
    main()




